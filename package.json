{
  "name": "perchance-ai-prompt-library",
  "version": "2.1.2",
  "description": "Complete prompt library and generator for Perchance AI tools, with API, Web, Discord, and browser extension integration (v2.0)",
  "main": "src/index.js",
  "bin": {
    "perchance-prompts": "bin/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "dev": "npm run api:dev",
    "generate": "node bin/cli.js",
    "batch": "node bin/cli.js batch",
    "export": "node bin/cli.js export",
    "test:batch": "jest tests/batch-generation.test.js",
    "test:export": "jest tests/export-functionality.test.js",
    "api": "node src/api/server.js",
    "api:dev": "nodemon src/api/server.js",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "discord:dev": "nodemon discord-bot/index.js"
  },
  "keywords": [
    "perchance",
    "ai",
    "prompt",
    "generator",
    "image-generation",
    "batch-generation",
    "export",
    "variations",
    "web-interface",
    "api",
    "discord-bot",
    "browser-extension",
    "style-mixing",
    "v2"
  ],
  "author": "AI Research Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "boxen": "^8.0.1",
    "canvas": "^3.1.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^9.4.1",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "enquirer": "^2.4.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "figlet": "^1.8.2",
    "fs-extra": "^11.3.0",
    "fuse.js": "^7.1.0",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^8.2.6",
    "jimp": "^1.6.0",
    "openai": "^5.11.0",
    "ora": "^8.2.0",
    "replicate": "^1.0.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "term-size": "^3.0.2",
    "wrap-ansi": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.3.1",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}

    "express-validator": "^7.2.0",
    "figlet": "^1.8.2",
    "gradient-string": "^3.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "react-markdown": "^9.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "term-size": "^4.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "wrap-ansi": "^9.0.0",
    "zod": "^3.23.8"
